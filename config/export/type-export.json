{
  "prefixes": {
    "mandaat": "http://data.vlaanderen.be/ns/mandaat#",
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "persoon": "http://data.vlaanderen.be/ns/persoon#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "org": "http://www.w3.org/ns/org#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "regorg": "https://www.w3.org/ns/regorg#",
    "person": "http://www.w3.org/ns/person#",
    "schema": "http://schema.org/",
    "prov": "http://www.w3.org/ns/prov#",
    "adms": "https://www.w3.org/ns/adms#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "types": [
    {
      "type": "mandaat:Mandataris",
      "optionalProperties": [
        "mandaat:start",
        "mandaat:einde",
        "mandaat:rangorde",
        "mandaat:status",
        "mandaat:isBestuurlijkeAliasVan",
        "mandaat:isTijdelijkVervangenDoor",
        "org:holds",
        "mandaat:beleidsdomein",
        "org:hasMembership"
      ],
      "additionalFilter": ""
    },
    {
      "type": "person:Person",
      "optionalProperties": [
        "foaf:familyName",
        "persoon:gebruikteVoornaam",
        "foaf:name",
        "adms:identifier",
        "owl:sameAs"
      ],
      "additionalFilter": ""
    },
    {
      "type": "person:Person",
      "optionalProperties": [
        "persoon:heeftGeboorte",
        "persoon:geslacht"
      ],
      "additionalFilter": "          ?resource ^mandaat:isBestuurlijkeAliasVan ?mandataris .\n          ?mandataris mandaat:start ?start .\n          ?mandataris mandaat:einde ?einde .\n          FILTER(?start > \"2012-01-01T00:00:00\"^^xsd:dateTime)          FILTER(?einde < \"2019-01-01T00:00:00\"^^xsd:dateTime)"
    },
    {
      "type": "mandaat:TijdsgebondenEntiteit",
      "optionalProperties": [
        "mandaat:bindingEinde",
        "mandaat:bindingStart",
        "mandaat:isTijdspecialisatieVan"
      ],
      "additionalFilter": ""
    },
    {
      "type": "mandaat:Fractie",
      "optionalProperties": [
        "regorg:legalName"
      ],
      "additionalFilter": ""
    },
    {
      "type": "besluit:Bestuurseenheid",
      "optionalProperties": [
        "skos:prefLabel",
        "besluit:werkingsgebied",
        "besluit:classificatie",
        "org:hasPrimarySite",
        "org:linkedTo",
        "schema:contactPoint",
        "org:heldBy"
      ],
      "additionalFilter": ""
    },
    {
      "type": "besluit:Bestuursorgaan",
      "optionalProperties": [
        "skos:prefLabel",
        "mandaat:bindingEinde",
        "mandaat:bindingStart",
        "besluit:bestuurt",
        "besluit:classificatie",
        "mandaat:isTijdspecialisatieVan",
        "org:hasPost"
      ],
      "additionalFilter": ""
    },
    {
      "type": "persoon:Geboorte",
      "optionalProperties": [
        "persoon:datum",
        "owl:sameAs"
      ],
      "additionalFilter": "          ?resource ^persoon:heeftGeboorte ?person .\n          ?person  ^mandaat:isBestuurlijkeAliasVan ?mandataris .\n          ?mandataris mandaat:start ?start .\n          ?mandataris mandaat:einde ?einde .\n          FILTER(?start > \"2012-01-01T00:00:00\"^^xsd:dateTime)          FILTER(?einde < \"2019-01-01T00:00:00\"^^xsd:dateTime)"
    },
    {
      "type": "org:Membership",
      "optionalProperties": [
        "org:organisation",
        "org:mumberDuring"
      ],
      "additionalFilter": ""
    },
    {
      "type": "mandaat:Mandaat",
      "optionalProperties": [
        "mandaat:aantalHouders",
        "org:role"
      ],
      "additionalFilter": ""
    },
    {
      "type": "skos:Concept",
      "optionalProperties": [
        "skos:prefLabel",
        "skos:scopeNote"
      ],
      "additionalFilter": ""
    }
  ]
}
